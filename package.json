{
  "name": "project-nghiepuit",
  "version": "1.1.1",
  "description": "A boilerplate for web APIs using Node",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": ">=7.6.0"
  },
  "scripts": {
    "start:dev": "cross-env NODE_PATH=. NODE_ENV=development nodemon",
    "start": "cross-env NODE_PATH=. NODE_ENV=production nodemon",
    "coverage": "cross-env NODE_PATH=. NODE_ENV=test nyc mocha --opts test/mocha.opts.unit",
    "lint": "eslint {src,test,config}/**/*.js",
    "sequelize": "cross-env NODE_PATH=. sequelize",
    "console": "cross-env NODE_PATH=. node src/interfaces/console/index.js",
    "heroku-postbuild": "NODE_ENV=production NODE_PATH=. sequelize db:migrate --url=$DATABASE_URL",
    "pm2": "pm2",
    "cleanup": "node scripts/cleanup.js",
    "db:refresh": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "db:refresh:production": "NODE_ENV=production sequelize db:migrate:undo:all && NODE_ENV=production sequelize db:migrate",
    "db:seed": "cross-env NODE_ENV=development NODE_PATH=. sequelize db:seed:all --seeders-path=./src/infra/database/seeds",
    "db:seed:production": "cross-env NODE_ENV=production NODE_PATH=. sequelize db:seed:all --seeders-path=./src/infra/database/seeds"
  },
  "repository": {},
  "author": "nghiepuit@gmail.com",
  "license": "MIT",
  "dependencies": {
    "awilix": "^3.0.9",
    "awilix-express": "^0.11.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.17.1",
    "composable-middleware": "^0.3.0",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "cross-env": "^3.2.3",
    "del": "^2.2.2",
    "dotenv": "^4.0.0",
    "eslint": "^4.7.2",
    "express": "^4.15.2",
    "express-status-monitor": "^0.1.9",
    "fs-extra": "^8.0.0",
    "http-status": "^1.0.1",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.4.0",
    "method-override": "^2.3.7",
    "morgan": "^1.8.1",
    "multer": "^1.4.1",
    "mysql": "^2.17.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^6.1.3",
    "pm2": "^2.4.2",
    "ramda": "^0.26.1",
    "rimraf": "^2.6.3",
    "sequelize": "^3.30.4",
    "sequelize-cli": "^3.0.0",
    "sqlite3": "^4.0.6",
    "structure": "^1.2.0",
    "swagger-ui-express": "^4.0.6",
    "tcomb": "^3.2.29"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-change": "^2.1.2",
    "chance": "^1.0.6",
    "dirty-chai": "^2.0.1",
    "factory-girl": "^4.0.0",
    "listr": "^0.11.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "nyc": "^11.2.1",
    "replace-in-file": "^2.5.0",
    "supertest": "^3.0.0",
    "supertest-as-promised": "^4.0.2"
  }
}
